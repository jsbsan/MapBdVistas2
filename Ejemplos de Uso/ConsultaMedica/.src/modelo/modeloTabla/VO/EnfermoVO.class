Public Function Copia(Optional dato As EnfermoVO) As EnfermoVO

  Dim datoVOtmp As New EnfermoVO
  Dim datoVOtmpOriginal As EnfermoVO

  If IsNull(dato) Then
    dato = Me 'se copia a si mismo
  Endif
  datoVOtmp.id = dato.id
  datoVOtmp.nombre = dato.nombre
  datoVOtmp.apellido = dato.apellido
  datoVOtmp.telefono = dato.telefono
  datoVOtmp.idenfermo = dato.idenfermo
  Return datoVOtmp

End

Public Function ConvertirVOaColeccion(dato As EnfermoVO) As Collection

  Dim colecciontmp As New Collection
  colecciontmp.add(dato.id, "id")
  colecciontmp.add(dato.nombre, "nombre")
  colecciontmp.add(dato.apellido, "apellido")
  colecciontmp.add(dato.telefono, "telefono")
  colecciontmp.add(dato.idenfermo, "idenfermo")
  Return colecciontmp

End

