Public Function ConvertirVOaColeccion(dato As CompactaVO) As Collection

  Dim colecciontmp As New Collection
  colecciontmp.add(dato.NombreCompleto, "NombreCompleto")
  colecciontmp.add(dato.email, "email")
  Return colecciontmp

End

Public Function ConvertirColeccionaVO(dato As Collection) As CompactaVO

  Dim datoVOtmp As New CompactaVO
Try datoVOtmp.NombreCompleto = dato["NombreCompleto"]
Try datoVOtmp.email = dato["email"]
Return datoVOtmp
End
